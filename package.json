  "bugs": {
    "url": "https://github.com/maescode/React-E-commerce-Website/issues"
  },
  "homepage": "https://github.com/maescode/React-E-commerce-Website#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "concurrently": "^6.4.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "mongoose": "^6.0.12",
    "nodemon": "^2.0.15"
  }
}
